{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-extra-semi": 0,
        "semi": 2,
        "no-unused-vars": 0,
        "quotes": ["error", "single"]
    }
}